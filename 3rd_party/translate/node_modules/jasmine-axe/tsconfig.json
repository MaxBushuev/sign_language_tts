{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "lib": ["es6", "dom"],
        "strict": true,
        "alwaysStrict": true,
        "declaration": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictFunctionTypes": true,
        "strictNullChecks": true,
        "types": [],
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true,
    },
    "include": [
        "./index.ts",
    ],
}
